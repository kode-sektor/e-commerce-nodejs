	<section id="best-sellers" class="best-seller product-listing">

		<div class="page-product">

			<h2 class="section-title">Show of Our Products by Category</h2>

			<ul class="category product-list">
				{{#each dataCat}}
				<li>
					<a href="/" style="display: block;">
						<div class="product-listing-single">
							<h4 class="category-title">({{this.category}})</h4>
						    <h5>{{this.title}}</h5>
							<h5>Price : $ CAD {{this.price}}</h5>
						    <figure>
						    	<img src="/img/{{this.imgPath}}">
						    	<figcaption>
						    		<p>{{this.description}}</p>
						    	</figcaption>
						    </figure>
						</div>
					</a>
				</li>
				{{/each}}
			</ul>

		</div>

	</section>

